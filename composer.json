{
    "name": "ageekdev/laravel-num",
    "description": "To convert the unicode digit to another unicode digit.",
    "keywords": [
        "laravel",
        "number",
        "unicode",
        "english",
        "myanmar",
        "thai"
      ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tint Naing Win",
            "email": "amigo.k8@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.1",
        "nunomaduro/larastan": "2.1.12",
        "orchestra/testbench": "^6.23|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AgeekDev\\Num\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AgeekDev\\Num\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "analyse-clear": "vendor/bin/phpstan clear-result-cache",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false,
            "phpstan/extension-installer": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgeekDev\\Num\\NumServiceProvider"
            ],
            "aliases": {
                "Nrc": "AgeekDev\\Num\\Facades\\Num"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
